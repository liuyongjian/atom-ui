{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "./components/*"
      ]
    },
    "jsx": "react-jsx",
    "types": [
      "react",
    ],
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": [
    "components/**/*",
    "global.d.ts"
  ]
}